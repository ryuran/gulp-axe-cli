{
  "name": "gulp-axe-cli",
  "version": "0.0.2",
  "description": "Gulp plugin to test static pages with aXe as pre-commit hook or in CI environment.",
  "license": "MIT",
  "repository": "https://github.com/samueleishion/gulp-axe-cli",
  "author": {
    "name": "Sam Acuna",
    "email": "samueleishion@gmail.com",
    "url": "samuelacuna.com"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "precommitmsg": "npm run lint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "prepush": "npm-run-all test",
    "lint": "standard ./**/*.js --fix",
    "test": "npm-run-all lint ava",
    "axe": "axe",
    "ava": "ava",
    "standard": "standard",
    "npm-run-all": "npm-run-all"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "gulpplugin"
  ],
  "dependencies": {
    "axe-cli": "3.0.0",
    "gulp-util": "3.0.8",
    "plugin-error": "0.1.2",
    "through2": "2.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "ava": "0.24.0",
    "husky": "0.14.3",
    "npm-run-all": "4.1.3",
    "p-event": "1.3.0",
    "standard": "11.0.1",
    "vinyl": "2.1.0"
  },
  "standard": {
    "ignore": [
      "node_modules",
      ".git"
    ]
  }
}
